  "vcl/vcl_config_headers.h~" ) CONFIG_FILES="$CONFIG_FILES vcl/vcl_config_headers.h~:vcl/vcl_config_headers.h.in" ;;
  "core/vxl_config.h~" ) CONFIG_FILES="$CONFIG_FILES core/vxl_config.h~:core/vxl_config.h.in" ;;
